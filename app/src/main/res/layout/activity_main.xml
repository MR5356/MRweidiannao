                    android:layout_height="wrap_content"
                    android:paddingBottom="15dp"
                    android:background="@drawable/btn_shape"
                    android:text="@string/nine_pic_info"
                    android:textSize="10sp"
                    android:textColor="@color/gray"
                    android:paddingLeft="30dp"
                    android:drawablePadding="20dp"
                    android:paddingStart="30dp"
                    tools:ignore="RtlSymmetry"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/time_display"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="2dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp"
                    android:paddingBottom="15dp"
                    android:background="@drawable/btn_shape"
                    android:text="@string/time_display"
                    android:textSize="13sp"
                    android:textColor="@color/blank"
                    android:drawableLeft="@drawable/ic_access_time_gray_24dp"
                    android:drawableRight="@drawable/ic_star_canuse_24dp"
                    android:paddingLeft="30dp"
                    android:paddingRight="30dp"
                    android:drawablePadding="20dp"
                    android:drawableStart="@drawable/ic_access_time_gray_24dp"
                    android:paddingStart="30dp"
                    tools:ignore="RtlSymmetry"
                    android:drawableEnd="@drawable/ic_star_canuse_24dp" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="15dp"
                    android:background="@drawable/btn_shape"
                    android:text="@string/time_display_info"
