<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF5F5F5"
    tools:context=".MainActivity"
    android:descendantFocusability="blocksDescendants">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        app:titleTextAppearance="@style/ToolbarTitle"
        app:subtitleTextAppearance="@style/ToolbarSubtitle"
        android:theme="?attr/actionBarTheme"
        tools:ignore="MissingConstraints" />

    <ScrollView
        android:id="@+id/scroll_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/toolbar">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
            android:id="@+id/about"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/about"
            android:textSize="15sp"
            android:textColor="@color/blank"
            android:gravity="center_vertical"
            android:drawableLeft="@drawable/ic_hdr_strong_gray_24dp"
            android:drawableRight="@drawable/ic_chevron_right_gray_24dp"
            android:paddingRight="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="10dp"
            android:drawablePadding="20dp"
            android:paddingLeft="20dp"
            android:background="@drawable/btn_shape"
            android:drawableStart="@drawable/ic_hdr_strong_gray_24dp"
            android:drawableEnd="@drawable/ic_chevron_right_gray_24dp"
            android:paddingEnd="10dp"
            android:paddingStart="20dp" />

            <Button
                android:id="@+id/dev_person"
                android:layout_marginTop="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/dev_person"
                android:textSize="15sp"
                android:textColor="@color/blank"
                android:gravity="center_vertical"
                android:drawableLeft="@drawable/ic_developer_mode_gray_24dp"
                android:drawableRight="@drawable/ic_chevron_right_gray_24dp"
                android:paddingRight="10dp"
                android:drawablePadding="20dp"
                android:paddingLeft="20dp"
                android:background="@drawable/btn_shape"
                android:drawableStart="@drawable/ic_developer_mode_gray_24dp"
                android:drawableEnd="@drawable/ic_chevron_right_gray_24dp"
                android:paddingEnd="10dp"
                android:paddingStart="20dp" />

            <Button
                android:id="@+id/old_version"
                android:layout_marginTop="8dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/old_version"
                android:textSize="15sp"
                android:gravity="center_vertical"
                android:background="@drawable/btn_shape"
                android:paddingRight="10dp"
                android:drawableRight="@drawable/ic_chevron_right_gray_24dp"
                android:drawableLeft="@drawable/ic_web_asset_gray_24dp"
                android:paddingLeft="20dp"
                android:drawablePadding="20dp"
                android:paddingEnd="10dp"
                android:drawableEnd="@drawable/ic_chevron_right_gray_24dp"
                android:drawableStart="@drawable/ic_web_asset_gray_24dp"
                android:paddingStart="20dp" />

            <Button
                android:id="@+id/clear_cache"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
